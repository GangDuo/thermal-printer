{
  "manifest_version": 2,
  "name": "GitHub Printer",
  "description": "",
  "version": "1.0.0",
  "icons": {
    "16": "images/icon/icon_16.png",
    "48": "images/icon/icon_48.png",
    "128": "images/icon/icon_128.png"
  },
  "background": {
    "scripts": [
      "lib/jquery-3.3.1.min.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon/icon_19.png",
      "38": "images/icon/icon_38.png"
    },
    "default_title": "GitHub Printer",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "lib/jquery-3.3.1.min.js",
        "js/github.js"
      ]
    }
  ],
  "permissions": [
    "clipboardWrite"
  ]
}
